{
  "manifest_version": 2,
  "name": "Go to github",
  "description": "A chrome extension that redirect from npm to github",
  "version": "1.0",
  "background": {
    "persistent": true,
    "page": "bg.html"
  },
  "permissions": [
    "tabs",
    "storage",
    "https://www.npmjs.com/package/*",
    "https://registry.npmjs.org/*"
  ]
}
